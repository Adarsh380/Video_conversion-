{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/video-converter.html",
      "dest": "/video-converter.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  }
}